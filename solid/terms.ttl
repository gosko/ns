@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix : <http://www.w3.org/2000/01/rdf-schema#>.
<>
    <http://creativecommons.org/ns#attributionName> "Andrei Sambra" ;
    <http://creativecommons.org/ns#attributionURL> <> ;
    <http://creativecommons.org/ns#license> <http://creativecommons.org/licenses/by-sa/2.0/uk/deed.en_GB> ;
    <http://purl.org/dc/terms/issued> "2015-11-16"^^<http://www.w3.org/2001/XMLSchema#date> ;
    a <http://www.w3.org/2002/07/owl#Ontology> ;
    :label "Solid terms ontolgy"@en ;
    <http://xmlns.com/foaf/0.1/maker> <https://deiu.me/profile#me> .

<#TypeIndex>
    a :Class ;
    :comment "A index of type registries for resources. Applications can register the RDF type they use and list them in the index resource."@en ;
    :isDefinedBy <> ;
    :label "Type index"@en .

<#Notification>
    a :Class ;
    <http://www.w3.org/2000/01/rdf-schema#subClassOf> <http://rdfs.org/sioc/ns#Post> ;
    :comment "A notification resource."@en ;
    :isDefinedBy <> ;
    :label "Notification"@en .

<#Inbox>
    a :Class ;
    :comment "A resource containing notifications."@en ;
    :isDefinedBy <> ;
    :label "Inbox"@en .

<#Timeline>
    a :Class ;
    :comment "A resource containing time ordered items and sub-containers.  Sub-containers may be desirable in file based systems to split the timeline into logical components e.g. /yyyy-mm-dd/ as used in ISO 8061."@en ;
    :isDefinedBy <> ;
    :label "Timeline"@en .

<#read>
    a rdf:Property ;
    :comment "Indicates if a message has been read or not. This property should have a boolean datatype."@en ;
    :isDefinedBy <> ;
    :domain <http://rdfs.org/sioc/ns#Post> ;
    :label "read"@en .

<#typeIndex>
    a rdf:Property ;
    :comment "Points to a TypeIndex resource."@en ;
    :range <#TypeIndex> ;
    :isDefinedBy <> ;
    :label "type index"@en .

<#inbox>
    a rdf:Property ;
    :comment "Inbox resource for notifications."@en ;
    :domain <http://www.w3.org/2002/07/owl#Thing> ;
    :range <#Inbox> ;
    :isDefinedBy <> ;
    :label "inbox"@en .

<#timeline>
    a rdf:Property ;
    :comment "Timeline for a given resource."@en ;
    :domain <http://www.w3.org/2002/07/owl#Thing> ;
    :isDefinedBy <> ;
    :label "timeline"@en .
