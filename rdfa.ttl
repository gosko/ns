@prefix dc: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ns3: <http://www.ivan-herman.net/foaf#> .
@prefix ns6: <http://www.ivan-herman.net/> .
@prefix ns7: <http://www.w3.org/TR/rdfa-core/#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfa: <http://www.w3.org/ns/rdfa#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xhv: <http://www.w3.org/1999/xhtml/vocab#> .

rdfa:PGClass a rdfs:Class,
        owl:Class;
    dc:description "is the top level class of the hierarchy" .

rdfa:PrefixOrTermMapping a rdfs:Class,
        owl:Class;
    dc:description "is the top level class for prefix or term mappings" .

rdfa: a owl:Ontology;
    dc:creator ns3:me;
    dc:date "2011-08-25";
    dc:description "This document describes the RDFa Vocabulary for Term and Prefix Assignment. The Vocabulary is used to modify RDFaÂ 1.1 processing behavior.";
    dc:title "RDFa Vocabulary for Term and Prefix Assignment, and for Processor Graph Reporting";
    rdfs:isDefinedBy ns7:s_profiles;
    owl:versionInfo "$Date$" .

rdfa:DocumentError dc:description "error condition; to be used when the document fails to be fully processed as a result of non-conformant host language markup";
    rdfs:subClassOf rdfa:Error,
        rdfa:PGClass .

rdfa:Info dc:description "is the class for all informations";
    rdfs:subClassOf rdfa:PGClass .

rdfa:ProfileReferenceError dc:description "error condition; to be used when a default RDFa Profile document fails to be retrieved and thus the document fails to be processed";
    rdfs:subClassOf rdfa:Error,
        rdfa:PGClass .

rdfa:UnresolvedCURIE dc:description "warning; to be used when a CURIE prefix fails to be resolved";
    rdfs:subClassOf rdfa:PGClass,
        rdfa:Warning .

rdfa:UnresolvedTerm dc:description "warning; to be used when a Term fails to be resolved";
    rdfs:subClassOf rdfa:PGClass,
        rdfa:Warning .

rdfa:VocabReferenceError dc:description "warning; to be used when the value of a @vocab attribute cannot be dereferenced, hence the vocabulary expansion cannot be completed";
    rdfs:subClassOf rdfa:PGClass,
        rdfa:Warning .

rdfa:context a rdf:Property,
        owl:ObjectProperty;
    dc:description "provides extra context for the error, eg, http response, an XPointer/XPath information, or simply the URI that created the error";
    rdfs:domain rdfa:PGClass .

rdfa:prefix a rdf:Property,
        owl:DatatypeProperty;
    dc:description "defines a prefix mapping for a URI; the value is supposed to be a NMTOKEN";
    rdfs:domain rdfa:PrefixMapping .

rdfa:term a rdf:Property,
        owl:DatatypeProperty;
    dc:description "defines a term mapping for a URI; the value is supposed to be a NMTOKEN";
    rdfs:domain rdfa:TermMapping .

rdfa:uri a rdf:Property,
        owl:DatatypeProperty;
    dc:description "defines the URI for either a prefix or a term mapping; the value is supposed to be an absolute URI";
    rdfs:domain rdfa:PrefixOrTermMapping .

rdfa:usesVocabulary a rdf:Property,
        owl:ObjectProperty;
    dc:description """provides a relationship between the host document and a vocabulary
	defined using the @vocab facility of RDFa1.1""" .

rdfa:vocabulary a rdf:Property,
        owl:DatatypeProperty;
    dc:description "defines an absolute URI to be used as a default vocabulary; the value is can be any string; for documentation purposes it is advised to use the string 'true' or 'True'." .

ns3:me a foaf:Person;
    rdfs:seeAlso ns6:foaf;
    foaf:mbox <mailto:ivan@w3.org>;
    foaf:name "Ivan Herman";
    foaf:title "Semantic Web Activity Lead";
    foaf:workplaceHomepage <http://www.w3.org> .

rdfa:PrefixMapping dc:description "is the class for prefix mappings";
    rdfs:subClassOf rdfa:PrefixOrTermMapping .

rdfa:TermMapping dc:description "is the class for term mappings";
    rdfs:subClassOf rdfa:PrefixOrTermMapping .

<http://www.w3.org/ns/rdfa.html> xhv:stylesheet <http://www.w3.org/StyleSheets/TR/base.css> .

rdfa:Error dc:description "is the class for all error conditions";
    rdfs:subClassOf rdfa:PGClass .

rdfa:Warning dc:description "is the class for all warnings";
    rdfs:subClassOf rdfa:PGClass .


