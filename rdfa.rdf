<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
  xmlns="file:///Users/ivan/W3C/WWW/ns/rdfa.ttl#"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:rdfa="http://www.w3.org/ns/rdfa#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
>
  <owl:FunctionalProperty rdf:about="http://www.w3.org/ns/rdfa#term">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#NMTOKEN"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain>
      <rdfs:Class rdf:about="http://www.w3.org/ns/rdfa#TermMapping">
        <rdfs:subClassOf>
          <rdfs:Class rdf:about="http://www.w3.org/ns/rdfa#VocabularyMapping"/>
        </rdfs:subClassOf>
      </rdfs:Class>
    </rdfs:domain>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="http://www.w3.org/ns/rdfa#uri">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/rdfa#VocabularyMapping"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="http://www.w3.org/ns/rdfa#prefix">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#NMTOKEN"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain>
      <rdfs:Class rdf:about="http://www.w3.org/ns/rdfa#PrefixMapping">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/rdfa#VocabularyMapping"/>
      </rdfs:Class>
    </rdfs:domain>
  </owl:FunctionalProperty>
  <owl:AllDisjointClasses>
    <owl:members rdf:parseType="Collection">
      <rdf:Description rdf:about="http://www.w3.org/ns/rdfa#PrefixMapping"/>
      <rdf:Description rdf:about="http://www.w3.org/ns/rdfa#TermMapping"/>
    </owl:members>
  </owl:AllDisjointClasses>
</rdf:RDF>
