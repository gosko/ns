<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
  xmlns:foaf="http://xmlns.com/foaf/0.1/"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:dc="http://purl.org/dc/terms/"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:rdfa="http://www.w3.org/ns/rdfa#"
  xmlns:xhv="http://www.w3.org/1999/xhtml/vocab#"
>
  <owl:Ontology rdf:about="http://www.w3.org/ns/rdfa#">
    <owl:versionInfo>$Date$</owl:versionInfo>
    <dc:description>This document describes the RDFa Vocabulary for Term and Prefix Assignment. The Vocabulary is used to modify RDFaÂ 1.1 processing behavior.</dc:description>
    <dc:creator>
      <foaf:Person rdf:about="http://www.ivan-herman.net/foaf#me">
        <foaf:mbox rdf:resource="mailto:ivan@w3.org"/>
        <foaf:workplaceHomepage rdf:resource="http://www.w3.org"/>
        <rdfs:seeAlso rdf:resource="http://www.ivan-herman.net/foaf"/>
        <foaf:title>Semantic Web Activity Lead</foaf:title>
        <foaf:name>Ivan Herman</foaf:name>
      </foaf:Person>
    </dc:creator>
    <dc:date>2011-11-22</dc:date>
    <dc:title>RDFa Vocabulary for Term and Prefix Assignment, and for Processor Graph Reporting</dc:title>
    <rdfs:isDefinedBy rdf:resource="http://www.w3.org/TR/rdfa-core/#s_initialcontexts"/>
  </owl:Ontology>
  <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/rdfa#prefix">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <dc:description>defines a prefix mapping for a URI; the value is supposed to be a NMTOKEN</dc:description>
    <rdfs:domain>
      <rdf:Description rdf:about="http://www.w3.org/ns/rdfa#PrefixMapping">
        <dc:description>is the class for prefix mappings</dc:description>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/rdfa#PrefixOrTermMapping"/>
      </rdf:Description>
    </rdfs:domain>
  </owl:DatatypeProperty>
  <rdf:Description rdf:about="http://www.w3.org/ns/rdfa.html">
	<xhv:stylesheet rdf:resource="http://www.w3.org/StyleSheets/TR/base.css"/>
  </rdf:Description>
  <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/rdfa#term">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <dc:description>defines a term mapping for a URI; the value is supposed to be a NMTOKEN</dc:description>
    <rdfs:domain>
      <rdf:Description rdf:about="http://www.w3.org/ns/rdfa#TermMapping">
        <dc:description>is the class for term mappings</dc:description>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/rdfa#PrefixOrTermMapping"/>
      </rdf:Description>
    </rdfs:domain>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/rdfa#uri">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <dc:description>defines the URI for either a prefix or a term mapping; the value is supposed to be an absolute URI</dc:description>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/rdfa#PrefixOrTermMapping"/>
  </owl:DatatypeProperty>
  <rdf:Description rdf:about="http://www.w3.org/ns/rdfa#UnresolvedTerm">
    <dc:description>warning; to be used when a Term fails to be resolved</dc:description>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/rdfa#PGClass"/>
    <rdfs:subClassOf>
      <rdf:Description rdf:about="http://www.w3.org/ns/rdfa#Warning">
        <dc:description>is the class for all warnings</dc:description>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/rdfa#PGClass"/>
      </rdf:Description>
    </rdfs:subClassOf>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/rdfa#VocabReferenceError">
    <dc:description>warning; to be used when the value of a @vocab attribute cannot be dereferenced, hence the vocabulary expansion cannot be completed</dc:description>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/rdfa#PGClass"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/rdfa#Warning"/>
  </rdf:Description>
  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/rdfa#usesVocabulary">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <dc:description>provides a relationship between the host document and a vocabulary
	defined using the @vocab facility of RDFa1.1</dc:description>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/rdfa#vocabulary">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <dc:description>defines an absolute URI to be used as a default vocabulary; the value is can be any string; for documentation purposes it is advised to use the string 'true' or 'True'.</dc:description>
  </owl:DatatypeProperty>
  <rdf:Description rdf:about="http://www.w3.org/ns/rdfa#Info">
    <dc:description>is the class for all informations</dc:description>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/rdfa#PGClass"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/rdfa#UnresolvedCURIE">
    <dc:description>warning; to be used when a CURIE prefix fails to be resolved</dc:description>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/rdfa#PGClass"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/rdfa#Warning"/>
  </rdf:Description>
  <owl:ObjectProperty rdf:about="http://www.w3.org/ns/rdfa#context">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <dc:description>provides extra context for the error, eg, http response, an XPointer/XPath information, or simply the URI that created the error</dc:description>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/rdfa#PGClass"/>
  </owl:ObjectProperty>
  <rdf:Description rdf:about="http://www.w3.org/ns/rdfa#DocumentError">
    <dc:description>error condition; to be used when the document fails to be fully processed as a result of non-conformant host language markup</dc:description>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/rdfa#PGClass"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/rdfa#Error"/>
  </rdf:Description>
  <owl:Class rdf:about="http://www.w3.org/ns/rdfa#PrefixOrTermMapping">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <dc:description>is the top level class for prefix or term mappings</dc:description>
  </owl:Class>
  <owl:Class rdf:about="http://www.w3.org/ns/rdfa#PGClass">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <dc:description>is the top level class of the hierarchy</dc:description>
  </owl:Class>
</rdf:RDF>

