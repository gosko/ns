  <owl:Ontology rdf:about="http://www.w3.org/ns/ssn/systems/">
    <rdf:type rdf:resource="http://purl.org/vocommons/voaf#Vocabulary"/>
    <dc:title xml:lang="en">System capabilities, operating ranges, and survival ranges ontology</dc:title>
    <dc:description xml:lang="en">This ontology describes system capabilities, operating ranges, and survival ranges.</dc:description>
    <dc:creator>
      <foaf:Agent>
        <foaf:name xml:lang="en">W3C/OGC Spatial Data on the Web Working Group</foaf:name>
      </foaf:Agent>
    </dc:creator>

    <dc:rights>Copyright 2017 W3C/OGC.</dc:rights>
    <dc:license rdf:resource="http://www.w3.org/Consortium/Legal/2015/copyright-software-and-document"/>
