<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
  <rdf:Description rdf:about="#atime">
    <rdfs:comment>time of last access</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="#blksize">
    <rdfs:comment>blocksize for file system I/O</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="#blocks">
    <rdfs:comment>number of 512B blocks allocated</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="#ctime">
    <rdfs:comment>time of last status change</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="#dev">
    <rdfs:comment>ID of device containing file</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="#gid">
    <rdfs:comment>group ID of owner</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="#ino">
    <rdfs:comment>inode number</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="#mode">
    <rdfs:comment>protection</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="#mtime">
    <rdfs:comment>time of last modification</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="#nlink">
    <rdfs:comment>number of hard links</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="#rdev">
    <rdfs:comment>device ID (if special file)</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="#size">
    <rdfs:comment>total size, in bytes</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="#uid">
    <rdfs:comment>user ID of owner</rdfs:comment>
  </rdf:Description>
</rdf:RDF>
