      <schema:affiliation rdf:parseType="Resource">
        <foaf:name>Digital Earth and Reference Data (DE) Unit, Institute for Environment and Sustainability (IES) of the EC DG Joint Research Centre (JRC)</foaf:name>
        <foaf:homepage rdf:resource="http://ies.jrc.ec.europa.eu/DE"/>
      </schema:affiliation>
    </dcterms:creator>
    <dcterms:creator rdf:parseType="Resource">
      <rdfs:seeAlso rdf:resource="http://philarcher.org/foaf.rdf#me"/>
