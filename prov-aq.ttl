    rdfs:isDefinedBy <http://www.w3.org/ns/prov#> .

rdfs:isDefinedBy
    a owl:AnnotationProperty .

rdfs:label
    a owl:AnnotationProperty ;
    rdfs:comment ""@en ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov#> .

rdfs:seeAlso
    a owl:AnnotationProperty ;
    rdfs:comment ""@en .

