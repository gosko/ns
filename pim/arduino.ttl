    @prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
    @prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
    
    <>     a owl:Ontology;
         dc:title "A simple arduino I/O vocabulary" .
    
    a:Snapshot     a :Class;
         :comment "A snapshot represents the state of the I/O pins of an Arduino";
         :label "snapshot" .
    
    a:a0     a rdf:Property;
         :domain a:Snapshot;
         :label "analog input 0";
         :range xsd:integer;
         a:pin 0 .
    
    a:a1     a rdf:Property;
         :domain a:Snapshot;
         :label "analog input 1";
         :range xsd:integer;
         a:pin 0 .
    
    a:a2     a rdf:Property;
         :domain a:Snapshot;
         :label "analog input 2";
         :range xsd:integer;
         a:pin 0 .
    
    a:a3     a rdf:Property;
         :domain a:Snapshot;
         :label "analog input 3";
         :range xsd:integer;
         a:pin 0 .
    
    a:a4     a rdf:Property;
         :domain a:Snapshot;
         :label "analog input 4";
         :range xsd:integer;
         a:pin 0 .
    
    a:a6     a rdf:Property;
         :domain a:Snapshot;
         :label "analog input 5";
         :range xsd:integer;
         a:pin 0 .
    
    a:alalogInput     a :Class;
         :subClassOf rdf:Property .
    
    a:d0     a rdf:Property;
         :domain a:Snapshot;
         :label "digital pin 0";
         :range xsd:integer;
