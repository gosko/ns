    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
  <owl:Ontology rdf:about="http://www.w3.org/ns/org#">
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date"
    >2012-10-06</dct:modified>
    <rdfs:comment xml:lang="en">Vocabulary for describing organizational structures, specializable to a broad variety of types of organization.</rdfs:comment>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date"
    >2014-01-25</dct:modified>
    <dct:title xml:lang="en">Core organization ontology</dct:title>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date"
    >2010-05-28</dct:created>
    <rdfs:comment xml:lang="it">Vocabolario per descrivere strutture organizzative, le quali possono essere specializzate in una vasta varietà di tipi di organizzazione</rdfs:comment>
    <dct:contributor rdf:parseType="Resource">
      <foaf:mbox>dave@epimorphics.com</foaf:mbox>
      <foaf:name>Dave Reynolds</foaf:name>
    </dct:contributor>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date"
    >2012-09-30</dct:modified>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date"
    >2014-01-02</dct:modified>
    <dct:contributor rdf:parseType="Resource">
      <foaf:mbox>antonio.maccioni@agid.gov.it</foaf:mbox>
      <foaf:name>Antonio Maccioni</foaf:name>
    </dct:contributor>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date"
    >2013-12-16</dct:modified>
    <dct:title xml:lang="it">Ontologia delle organizzazioni</dct:title>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date"
    >2013-02-15</dct:modified>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date"
    >2010-10-08</dct:modified>
    <rdfs:seeAlso rdf:resource="http://www.w3.org/TR/vocab-org/"/>
    <rdfs:label xml:lang="en">Core organization ontology</rdfs:label>
    <dct:contributor rdf:parseType="Resource">
      <foaf:mbox>dguardiola@quinode.fr</foaf:mbox>
      <foaf:name>Dominique Guardiola</foaf:name>
    </dct:contributor>
    <dct:contributor rdf:parseType="Resource">
      <foaf:mbox>giorgia.lodi@agid.gov.it</foaf:mbox>
      <foaf:name>Giorgia Lodi</foaf:name>
    </dct:contributor>
