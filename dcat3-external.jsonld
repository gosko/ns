  }, {
    "@id" : "dcterms:type",
    "skos:definition" : [ {
      "@language" : "cs",
      "@value" : "Podstata či žánr zdroje."
    }, {
      "@language" : "es",
      "@value" : "La naturaleza, género o tipo del recurso."
