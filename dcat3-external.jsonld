    }
  }, {
    "@id" : "dcterms:identifier",
    "skos:definition" : [ {
      "@language" : "es",
      "@value" : "En el contexto de DCAT, un identificador único para el recurso."
    }, {
      "@language" : "en",
      "@value" : "En el contexto de DCAT, un identificador único del recurso que se describe o cataloga."
    }, {
      "@language" : "it",
      "@value" : "Nel contesto del DCAT, un identificatore unico della risorsa descritta o catalogata."
    }, {
      "@language" : "it",
      "@value" : "Un riferimento univoco alla risorsa in un dato contesto."
    }, {
      "@language" : "cs",
      "@value" : "V kontextu DCAT, unikátní identifikátor položky."
    }, {
      "@language" : "en",
      "@value" : "An unambiguous reference to the resource within a given context."
    } ],
    "skos:editorialNote" : {
      "@language" : "en",
